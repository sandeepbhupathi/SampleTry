package com.operations.client.dto;

public class OperationsResponse {
	private String operationName;
	private int noOfOperations;
	private int finalOperationValue;
	public OperationsResponse(String operationName, int noOfOperations, int finalOperationValue) {
		this.operationName = operationName;
		this.noOfOperations = noOfOperations;
		this.finalOperationValue = finalOperationValue;
	}
	
	

}
